{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["local>ocofaigh/common-renovate:commonRenovateConfig"],
  "group": true,
  "groupName": "all",
  "regexManagers": [
    {
      "fileMatch": ["^.github/workflows/common.yml$"],
      "matchStrings": [
        "image:(?<depName>.*?)@(?<currentDigest>sha256:[a-f0-9]+)s"
      ],
      "datasourceTemplate": "docker"
    }
  ]
}
